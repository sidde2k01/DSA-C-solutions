#include <iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    for(int i=2;i<=n;i++){
        int check=i;
        bool flag=0;
        for(int j=2;j<check;j++){
            if(check%j==0){
                flag=1;
                break;
            }
        }
        if(flag==0){
            cout<<i<<endl;
        }
    }
  
}


