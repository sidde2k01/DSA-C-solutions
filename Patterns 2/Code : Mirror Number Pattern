#include<iostream>
using namespace std;


int main(){
    int n;
    cin>>n;
    int i=1;
    while(i<=n){
        int k=n-i;
        int count=1;
        while(count<=k){
            cout<<" ";
            count++;
        }
        int j=1;
        while(j<=i){
            cout<<j;
            j++;
        }
        cout<<endl;
        i++;
    }
  
}


