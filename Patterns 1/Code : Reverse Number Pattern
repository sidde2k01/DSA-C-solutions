#include<iostream>
using namespace std;


int main(){

     int n;
    cin>>n;
    int i=1;
    int count=1;
    while(i<=n){
        int k=1;
        int j=i;
        while(k<=count){
            cout<<j;
            j--;
            k++;
        }
        cout<<endl;
        count++;
        i++;
    }
  
}


