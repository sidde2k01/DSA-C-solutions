#include<iostream>
using namespace std;


int main(){

     int n;
    cin>>n;
    int i=1;
    int count=1;
    while(i<=n){
        count=1;
        int val=i;
        while(count<=i){
            cout<<val;
            val++;
            count++;
        }
        cout<<endl;
        i++;
    }
  
}


